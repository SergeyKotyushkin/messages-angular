{
  "name": "messages-angular",
  "version": "0.0.1",
  "description": "A solution for messages exchanging based on the \"nodejs-server-base\" solution with the angular at frontend.",
  "main": "./server/startup/index.js",
  "scripts": {
    "startDev": "nf start clean && nf start compile && nf start bundle && concurrently \"nf start compile-and-watch\" \"nf start server-dev\"",
    "start": "nf start clean && nf start compile && nf start bundle && nf start server",
    "compile": "nf start compile",
    "create-bundle": "nf start clean && nf start bundle",
    "bundle": "nf start clean && nf start compile && nf start bundle",
    "clean": "nf start clean"
  },
  "keywords": [
    "nodejs",
    "server",
    "messages",
    "chat",
    "angular2+"
  ],
